{
    "name": "antres/client-json-to-db",
    "description": "A generic client to map a json document in db",
    "type": "project",
    "require": {
        "symfony/flex": "^1.9",
        "composer/package-versions-deprecated": "^1.10",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "symfony/workflow": "^5.1",
        "symfony/http-client": "^5.1",
        "php": "^7.2",
        "symfony/dotenv": "^5.1",
        "symfony/yaml": "^5.1",
        "symfony/console": "^5.1",
        "symfony/monolog-bundle": "^3.5"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.20",
        "ext-pdo_sqlite": "^7.2",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/var-dumper": "^5.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Alberto Clemente",
            "email": "albertoclemente1983@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
